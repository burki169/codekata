#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/burki/projects/codepuzzles/rev-words-in-a-string/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/burki/projects/codepuzzles/rev-words-in-a-string/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../nopt@1.0.10/node_modules/nopt/bin/nopt.js" "$@"
else
  exec node  "$basedir/../../../../../nopt@1.0.10/node_modules/nopt/bin/nopt.js" "$@"
fi
